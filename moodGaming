void intro_border()
{
     int i,j;
 //Top border line...
 setxy(15,3);
 for(j=0; j<90; j++)
  printf("%c", 223);

 //Bottom border line...
 setxy(15,20);
 for(j=0; j<=90; j++)
  printf("%c", 223);

 //Left and Right border line...
 for(j=0; j<22; j++)
 {
  setxy(100,1+j);
  printf("%c",219);

  setxy(20,1+j);
  printf("%c",219);
 }
 printf("\n");
 return 0;
}

void intro_colors()
{
    while(i<4)
    {
        system("color 9");
        waitL();
        system("color 1");
        waitL();
        system("color 9");
        waitL();
        system("color A");
        waitL();
        system("color 2");
        waitL();
        system("color A");
        waitL();
        system("color B");
        waitL();
        system("color 3");
        waitL();
        system("color B");
        waitL();
        system("color C");
        waitL();
        system("color 4");
        waitL();
        system("color C");
        waitL();
        system("color D");
        waitL();
        system("color 5");
        waitL();
        system("color D");
        waitL();
        system("color E");
        waitL();
        system("color 6");
        waitL();
        system("color E");
        waitL();
        system("color F");
        i++;
    }
}

void intro()
{
    int choice;
        printf("\n\n\n\n\t\t\t\t               Welcome to mood gaming.\n");
        printf("\n\t\t\t\tThis game allows you to choose between 4 games to play: \n\t\t\t\t\t\t--->Snake\n\t\t\t\t\t\t--->Tic Tac Toe\n\t\t\t\t\t\t--->Hangman\n\t\t\t\t\t\t--->2048.\n");
        printf("\n\t\t\t\t  Depending on your mood you will be directed to play a game.\n");
        printf("\n\t\t\t     The objective is to improve your mood at the end of the game. \n");
        printf("\n\t\t\t\t\t Warning: You can only play one game at a time.    \n");
        printf("\n\t\t\t\t              Choose wisely. Good luck!                         \n");
        intro_border();
        intro_colors();
        getch();
        system("cls");
}

void intro_2()
{
    int choice;
    system("cls");
    system("color 3");
        printf("\n\n\n\n\n\t\t\t\xdb How are you feeling? \xdb\n");
        printf("\n\t\t\t\t\t\t-> COMPETITIVE\n");
        printf("\n\t\t\t\t\t\t-> BORED\n");
        printf("\n\t\t\t\t\t\t-> STRESSED\n");
        printf("\n\t\t\t\t\t\t-> ANGRY\n");
        printf("\n\t\t\t\t\t\t-> Exit game\n");
        printf("\n\t\t\t\t\t\tEnter numbers 1 to 5: ", choice);
        intro_border();
        intro_colors();
        getch();
}

//MAIN
int main()
{
    gname();
    intro();
    int choice;
    intro_2();
    do
    {
        scanf("%d", &choice);
        switch(choice)
        {
        case 1:
                {
                system("cls");
                system("color 2");
                printf("\n\n\t\t\t\t\t\xdb  YOUR CHOICE IS.......!!  \xdb\n");
                system("color 2");
                t_tname();
                wait();
                tic_tac_toe();
                break;
                }


        case 2:
               {
                system("cls");
                system("color 9");
                printf("\n\n\t\t\t\t\t\xdb  YOUR CHOICE IS.......!!  \xdb\n");
                sname();
                wait();
                system("cls");
                snake();
                break;
               }
        case 3:
                {
                    system("cls");
                    system("color 4");
                    printf("\n\n\t\t\t\t\t\xdb  YOUR CHOICE IS.......!!  \xdb\n");
                    tname();
                    wait();
                    system("cls");
                    twenty_forty_eight();
                    break;
                }
        case 4:
                {
                    system("cls");
                    system("color 6");
                    printf("\n\n\t\t\t\t\t\xdb  YOUR CHOICE IS.......!!  \xdb\n");
                    hname();
                    wait();
                    system("cls");
                    hangman();
                    break;
                }

        case 5:
                exit(0);
                break;

        default:
                printf("\n\n\n\t\t\tINVALID INPUT!!! Try again...");

        }
    }
    while(1);

}

